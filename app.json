{
  "expo": {
    "name": "CV Cloud",
    "slug": "cv-cloud-mobile-rebuild",
    "version": "2.0.5",
    "orientation": "portrait",
    "icon": "./assets/appIcon-1024.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#232936",
    "primaryColor": "#3498db",
    "description": "Professional CV builder and manager. Create, customize and share your CV in minutes with multiple modern templates, video introductions, and easy document management.",
    "splash": {
      "image": "./assets/logo-3000.png",
      "resizeMode": "contain",
      "backgroundColor": "#232936"
    },
    "ios": {
      "bundleIdentifier": "com.cvcloud.app",
      "buildNumber": "3",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "CV Cloud needs camera access to record video introductions and capture profile photos for your CV.",
        "NSMicrophoneUsageDescription": "CV Cloud needs microphone access to record audio for your video introduction.",
        "NSPhotoLibraryUsageDescription": "CV Cloud needs access to your photo library to select images for your CV profile picture.",
        "NSPhotoLibraryAddUsageDescription": "CV Cloud needs permission to save your CV documents to your photo library."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.cvcloud.app",
      "versionCode": 16,
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcon-adaptive-padded.png",
        "backgroundColor": "#232936"
      },
      "statusBar": {
        "barStyle": "light-content",
        "backgroundColor": "#232936"
      },
      "navigationBar": {
        "barStyle": "light-content",
        "backgroundColor": "#232936"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "cvcloud.com"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "newArchEnabled": true,
    "sdkVersion": "54.0.0",
    "extra": {
      "eas": {
        "projectId": "db713e3a-0eae-4c00-b458-aac92590a18e"
      }
    },
    "owner": "nicorapelas"
  }
}
